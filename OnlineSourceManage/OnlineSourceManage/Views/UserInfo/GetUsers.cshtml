@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title></title>
    <link rel="stylesheet" href="/Plugins/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/Plugins/easyui/easyui.css" />
    <link rel="stylesheet" href="/Plugins/font-awesome/css/font-awesome.min.css" />
    <link rel="stylesheet" href="/Css/eduOnline.css" />
    <!--[if lt IE 9]>
    <script type="text/javascript" src="/Plugins/ieCompatibility/html5.min.js"></script>
    <script type="text/javascript" src="/Plugins/ieCompatibility/respond.js"></script>
    <![endif]-->
</head>
<body class="layout">
    <input type="hidden" id="gridHeight" />
    <div class="container-fluid " data-option="region:center">
        <div class="row bg-grey">
            <div class="col-md-12">
                <div class="form-wrapper " id="btn-wrapper">

                    <input class="form-control input-inline" id="searchVal" placeholder="用户名、所在地、角色" />
                    <button id="searchUser" class="btn w-success vertical-top">查询</button>

                    <a class="btn w-success vertical-top pull-right ml-5" id="outport" href="/UserInfo/OutToExcel">导出Excel</a>
                     <button class="btn w-success vertical-top pull-right" id="viewExcel" >预览EXCEL报表</button>
                   
                    
                    <button class="btn w-success vertical-top" id="addUser">新增用户</button>
                </div>
            </div>
        </div>
        <div class="row mt-5">
            <div class="col-md-6 box-wrapper">
                <div class="chart" id="linechart"></div>
            </div>
            <div class="col-md-3 box-wrapper">
                <div class="chart" id="piechart-role"></div>
            </div>
            <div class="col-md-3 box-wrapper">
                <div class="chart" id="piechart-sex"></div>
            </div>
        </div>
        <div class="row ">
            <div class="col-md-12 mt-5">
                <div class="grid-wrapper" id="content-filed">
                    <div class="wrapper-box">
                        <table id="dataList"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>

</body>
<script src="~/Scripts/Js/common/jquery-1.10.1.js"></script>
<script src="~/Plugins/easyui/js/jquery.easyui.min.js"></script>
<script src="~/Plugins/easyui/js/easyui-lang-zh_CN.js"></script>
<script src="~/Plugins/layer/layer.js"></script>
<script src="~/Plugins/echarts/echarts.min.js"></script>
<script src="~/Scripts/Js/UserManage.js"></script>
</html>
